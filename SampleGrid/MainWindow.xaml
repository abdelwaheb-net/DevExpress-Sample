<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SampleGrid"
        xmlns:view="clr-namespace:SampleGrid.View"
         xmlns:viewModel="clr-namespace:SampleGrid.ViewModel"
    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
    xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
    xmlns:dxwuin="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/navigation"
        xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon" x:Class="SampleGrid.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <viewModel:MainViewModel/>
    </Window.DataContext>

    <Grid Background="Transparent">
            <dxmvvm:Interaction.Behaviors>
                <dxwuin:FrameNavigationService  Frame="{Binding Source={x:Reference frame}}" />
            </dxmvvm:Interaction.Behaviors>
            <dxwui:NavigationFrame x:Name="frame">
            <view:GridLinks></view:GridLinks>
        </dxwui:NavigationFrame>
        </Grid>
</Window>
    
